{
  "name": "Artwork",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "image": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true,
      "default": "Untitled"
    }
  },
  "validations": [],
  "relations": {
    "artist": {
      "type": "belongsTo",
      "model": "Artist",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": {}
}
